<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <declare-styleable name="RoundConner">
        <attr name="round_conner" format="dimension"/>
        <attr name="left_top_conner" format="dimension"/>
        <attr name="right_top_conner" format="dimension"/>
        <attr name="left_bottom_conner" format="dimension"/>
        <attr name="right_bottom_conner" format="dimension"/>
    </declare-styleable>

</resources>
